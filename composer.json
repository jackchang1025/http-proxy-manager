{
    "name": "weijiajia/http-proxy-manager",
    "description": "A Laravel package for managing HTTP proxies from various proxy service providers with a unified API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http",
        "proxy",
        "laravel",
        "php",
        "proxy-manager",
        "huashengdaili",
        "stormproxies",
        "ip-royal",
        "oxylabs",
        "proxyscrape"
    ],
    "homepage": "https://github.com/weijiajia/http-proxy-manager",
    "autoload": {
        "psr-4": {
            "Weijiajia\\HttpProxyManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weijiajia\\HttpProxyManager\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "weijiajia",
            "email": "shadowmatthew1025@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0",
        "illuminate/collections": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "symfony/var-dumper": "^6.0|^7.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "laravel/pint": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weijiajia\\HttpProxyManager\\ProxyManagerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
